@model MvcLabManager.Models.Lab

@{
    ViewData["Title"] = $"Lab {Model.Id}";
}

<h1>@ViewData["Title"]</h1>

<p><b>Id: </b> @Model.Id</p>
<p><b>Number: </b> @Model.Number</p>
<p><b>Name: </b> @Model.Name</p>
<p><b>Block: </b> @Model.Block</p>

<a class="btn btn-primary" asp-action="Update" asp-route-id="@Model.Id">Editar</a>
<a class="btn btn-danger" asp-action="Delete" asp-route-id="@Model.Id">Excluir</a>